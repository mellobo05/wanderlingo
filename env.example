# Translation API Keys (optional - app works without them)
# Get your API key from: https://console.cloud.google.com/apis/credentials
VITE_GOOGLE_TRANSLATE_API_KEY=your_google_translate_api_key_here

# Azure Translator (optional)
# Get your key from: https://portal.azure.com/
VITE_AZURE_TRANSLATOR_KEY=your_azure_translator_key_here
VITE_AZURE_TRANSLATOR_REGION=your_azure_region_here

# LibreTranslate URL (optional - defaults to https://libretranslate.com)
VITE_LIBRETRANSLATE_URL=https://libretranslate.com





